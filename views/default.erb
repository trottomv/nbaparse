<body>

<h1>NBA GAMES PARSE</h1>
<form action="/nba/all">
      <input id="clickall" type="submit" value="Last 10 NBA Games" onclick="showLoaderAll()"/>
      <img id="all" class="hidden" src="/ajax-loader.gif" />
</form>
<form action="/nba/hr">
      <input id="clickhr" type="submit" value="Huston Rockets" />
      <img id="hr" class="hidden" src="/ajax-loader.gif" />
</form>


</body>

<script type="text/javascript">
function showLoaderAll() {
  $("#all").removeClass("hidden");
  $("#hr").addClass("hidden");
}

function showLoaderHR() {
  $("#hr").removeClass("hidden");
  $("#all").addClass("hidden");
}
document.getElementById("clickall").onclick = showLoaderAll;
document.getElementById("clickhr").onclick = showLoaderHR;
</script>
